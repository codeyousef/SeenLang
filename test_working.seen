// Test core working features

// Variables and arithmetic
let x = 10
let y = 20
let sum = x + y

// While loop
var counter = 0
while counter < 5 {
    counter = counter + 1
}

// For loop
var total = 0
for i in 1..5 {
    total = total + i
}

// Arrays
let numbers = [1, 2, 3, 4, 5]
let first = numbers[0]

// Final calculation
sum + total + counter + first
