// Test modulo and fizzbuzz logic

func main() {
    val num = 15;
    
    // Test modulo operator
    val mod3 = num % 3;
    val mod5 = num % 5;
    val mod15 = num % 15;
    
    println(mod3);  // Should be 0
    println(mod5);  // Should be 0  
    println(mod15); // Should be 0
    
    // Basic fizzbuzz logic test
    if mod15 == 0 {
        println(99);  // FizzBuzz case
    }
}
