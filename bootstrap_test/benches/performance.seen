#[bench]
fun bench_arithmetic_operations() {
    let iterations = 10000;
    let mut sum = 0;
    let mut i = 0;
    
    while i < iterations {
        sum = sum + (i * 2);
        i = i + 1;
    }
    
    println("Arithmetic benchmark completed");
}

#[bench] 
fun bench_function_calls() {
    let iterations = 1000;
    let mut i = 0;
    
    while i < iterations {
        calculate_result(i);
        i = i + 1;
    }
    
    println("Function call benchmark completed");
}

#[bench]
fun bench_string_operations() {
    let iterations = 1000;
    let mut i = 0;
    
    while i < iterations {
        let message = "Benchmark iteration";
        println(message);
        i = i + 1;
    }
    
    println("String operations benchmark completed");
}

fun calculate_result(value: i32) -> i32 {
    let multiplier = 42;
    let result = value * multiplier;
    return result + 100;
}