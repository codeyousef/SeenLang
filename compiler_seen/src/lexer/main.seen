// Simplified Self-hosted Seen Lexer

// Capitalization-based visibility: SeenLexer (uppercase) = public
class SeenLexer {
    // New (uppercase) = public constructor
    fun New() -> SeenLexer {
        println("Creating SeenLexer");
    }
}

// Vec (uppercase) = public class
class Vec {
    // New (uppercase) = public constructor
    fun New() -> Vec {
        println("Creating Vec");
    }
}