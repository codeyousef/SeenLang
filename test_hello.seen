// Simple test program using Seen syntax

fun main() -> Int {
    // Test string interpolation
    let name = "Seen";
    println("Hello from {name}!");
    
    // Test word operators
    let x = 10;
    let y = 20;
    
    if x < y and y > 15 {
        println("Word operators work!");
    }
    
    // Test nullable types
    let maybe: String? = null;
    let display = maybe ?: "default";
    println("Nullable test: {display}");
    
    // Test range
    for i in 1..5 {
        println("Count: {i}");
    }
    
    return 0;
}