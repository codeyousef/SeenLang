// Test comprehensive Syntax Design features
fun main() {
    let greeting = "Hello, World!"
    let number = 42
    let condition = true
    
    if condition and number > 0 {
        let result = match number {
            1..10 -> "small"
            11..50 -> "medium" 
            _ -> "large"
        }
        
        let interpolated = "{greeting} Number is {result}!"
        println(interpolated)
        return null
    }
}
