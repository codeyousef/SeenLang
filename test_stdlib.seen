func test_math() -> Int {
    val a = abs_int(-42)         // Should return 42
    val b = min_int(10, 20)      // Should return 10
    val c = max_int(15, 25)      // Should return 25
    
    return a + b + c             // Should return 42 + 10 + 25 = 77
}

func test_float_math() -> Float {
    val f = abs_float(-3.14)     // Should return 3.14
    val p = pow_float(2.0, 3.0)  // Should return 8.0
    
    return f + p                 // Should return 11.14
}

func main() -> Int {
    val result = test_math()
    println("Math test result: ", result)
    return result
}