// Simple Seen Language Showcase
// Demonstrating working self-hosting capability

fun main() -> Int {
    let result = calculateValue(10, 5)
    return result
}

fun calculateValue(a: Int, b: Int) -> Int {
    if a > b {
        return a + b
    } else {
        return a - b
    }
}

fun greet(name: String) -> String {
    return "Hello, " + name
}